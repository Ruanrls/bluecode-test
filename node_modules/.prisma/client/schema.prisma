datasource db {
  provider = "sqlite"
  url      = "file:../../../dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model ShortenedUrl {
  id        String   @id @default(uuid())
  
  originalUrl       String @unique
  shortenedUrl      String @unique
  accessCount       Int  @default(0)

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}